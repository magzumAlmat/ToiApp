{
  "expo": {
    "sdkVersion": "52.0.0",
    "scheme": "myapp",
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amagzum.mobile",
      "infoPlist": {
        "NSCalendarsFullAccessUsageDescription": "Это приложение требует доступ к вашему календарю для добавления событий, связанных с вашей свадьбой.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor":"./assets/adaptive-icon.png"
      },
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "package": "com.amagzum.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-video",
      [
        "expo-calendar",
        {
          "isEnabled": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7eba8c74-3bb1-4e75-bcfe-079db935aaa5"
      }
    }
  }
}
